{
    "name": "hyperf/model-cache",
    "description": "A model cache component for hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "model-cache"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "psr/simple-cache": "^1.0",
        "hyperf/db-connection": "dev-master"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\ModelCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\ModelCache\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
